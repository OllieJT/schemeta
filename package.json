{
	"name": "schemeta",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/olliejt/schemeta.git",
		"directory": "cli"
	},
	"scripts": {
		"build": "vite build && npm run package",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"format:check": "prettier . --check --ignore-unknown --no-error-on-unmatched-pattern && manypkg check",
		"format": "prettier . --write --ignore-unknown --no-error-on-unmatched-pattern && manypkg fix",
		"lint:fix": "eslint --fix .",
		"lint": "eslint .",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublish": "npm run package",
		"preview": "vite preview",
		"release": "changeset version",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.7",
		"@manypkg/cli": "^0.21.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^5.0.0-next.183",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.11",
		"vitest": "^1.2.0"
	},
	"packageManager": "pnpm@8.15.6",
	"engines": {
		"node": ">=18.17.0"
	}
}
