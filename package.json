{
	"name": "schemeta",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/olliejt/schemeta.git",
		"directory": "cli"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"check": "pnpm run format:check && pnpm run typecheck && pnpm run lint",
		"format:check": "prettier . --check --ignore-unknown --no-error-on-unmatched-pattern && manypkg check",
		"format": "prettier . --write --ignore-unknown --no-error-on-unmatched-pattern && manypkg fix",
		"lint:fix": "eslint --fix .",
		"lint": "eslint .",
		"package": "publint",
		"preview": "vite preview",
		"release": "changeset version",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"README.md",
		"LICENSE",
		"CHANGELOG.md",
		"package.json"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"packageManager": "pnpm@8.15.6",
	"engines": {
		"node": ">=18.17.0"
	},
	"dependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.7",
		"@ianvs/prettier-plugin-sort-imports": "^4.2.1",
		"@manypkg/cli": "^0.21.4",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "^8.56.7",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.14.10",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"publint": "^0.1.9",
		"tslib": "^2.4.1",
		"tsup": "^8.1.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.11",
		"vitest": "^1.2.0"
	}
}
